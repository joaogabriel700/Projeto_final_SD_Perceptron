`timescale 1 ps/ 1 ps
module rom_3x16_vlg_sample_tst(
	adress,
	sampler_tx
);
input [1:0] adress;
output sampler_tx;

reg sample;
time current_time;
always @(adress)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

