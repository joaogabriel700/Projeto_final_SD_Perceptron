module rom_3x16_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [1:0] adress;
// wires                                               
wire [15:0] dado_out;

wire sampler;                             

// assign statements (if any)                          
rom_3x16 i1 (
// port map - connection between master ports and signals/registers   
	.adress(adress),
	.dado_out(dado_out)
);
// adress[ 1 ]
initial
begin
	adress[1] = 1'b1;
	# 50000;
	repeat(9)
	begin
		adress[1] = 1'b0;
		adress[1] = #50000 1'b1;
		# 50000;
	end
	adress[1] = 1'b0;
end 
// adress[ 0 ]
initial
begin
	adress[0] = 1'b1;
	# 50000;
	repeat(9)
	begin
		adress[0] = 1'b0;
		adress[0] = #50000 1'b1;
		# 50000;
	end
	adress[0] = 1'b0;
end 

rom_3x16_vlg_sample_tst tb_sample (
	.adress(adress),
	.sampler_tx(sampler)
);

rom_3x16_vlg_check_tst tb_out(
	.dado_out(dado_out),
	.sampler_rx(sampler)
);
endmodule

